{
  "name": "@nmarks/react-flexlayout",
  "version": "0.0.0",
  "description": "React responsive layout helpers",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean:dist && babel lib -d dist",
    "build:dev": "nodemon -w lib --exec \"npm run build\"",
    "clean": "npm run clean:dist && npm run clean:coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "coverage": "npm run clean:coverage && babel-node node_modules/.bin/isparta cover --report lcov --report text --report html test/index.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "nodemon -w lib -w test --exec \"npm run -s lint && npm run -s test:dev\"",
    "lint": "eslint lib",
    "precommit": "npm run docs",
    "prepublish": "in-publish && npm run build && npm run docs || not-in-publish",
    "test": "babel-node test/index.js",
    "test:dev": "npm run -s test | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathanmarks/react-flexlayout.git"
  },
  "keywords": [],
  "author": "Nathan Marks <info@nathanmarks.io> (http://nathanmarks.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nathanmarks/react-flexlayout/issues"
  },
  "homepage": "https://github.com/nathanmarks/react-flexlayout#readme",
  "devDependencies": {
    "app-module-path": "^1.0.5",
    "babel-cli": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "blue-tape": "^0.1.11",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^3.16.1",
    "isparta": "^4.0.0",
    "react-addons-test-utils": "^0.14.7",
    "rimraf": "^2.5.1",
    "tap-spec": "^4.1.1",
    "tap-xunit": "^1.3.1",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "in-publish": "^2.0.0",
    "lodash": "^4.2.1",
    "react": "^0.14.7"
  }
}
